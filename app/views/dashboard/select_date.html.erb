<div class="container">
  
  <div class="row">
    <div class="col-lg-12">
      <div class="alert alert-success"> Selected Date: <%=  "#{params[:search][:date_from]}" %> </div>
      <div class="alert alert-success">Compared to : <%= "#{params[:search][:date_to]}" %></div>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>Overview</h5>
        </div>
        <div class="panel-body">
          <%= render "/shared/table_overview" %>
        </div>
      </div>
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>Top Keywords (top 10 based on clicks)</h5>
        </div>
        <!-- /.panel-heading -->
        <div class="panel-body">
          <%= render "/shared/table_top_keywords" %>
          <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
   </div>
 </div>
  
  <div class="row">
    <div class="col-md-6">
     <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>New Keywords (top 10 based on impressions)</h5>
        </div>
        <div class="panel-body">
           <%= render "/shared/table_top_keywords_new_impressions" %>
           <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
        <h5>Keywords disappeared (top 10 based on impressions from past period)</h5>
        </div>
      <div class="panel-body">
        <%= render "/shared/table_top_keywords_past_impressions" %>
        <a href="#" class="btn btn-primary">View More Link</a>
      </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-6">
     <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>Improved Rankings (only displaying queries with more than 100 impressions) (top 10 based on average position improvement)</h5>
        </div>
        <div class="panel-body">
           <%= render "/shared/improved_rankings" %>
           <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
         <h5>Lost Rankings (only displaying queries with more than 100 impressions) (top 10 based on average position improvement)</h5>
        </div>
      <div class="panel-body">
         <%= render "/shared/lost_rankings" %>
         <a href="#" class="btn btn-primary">View More Link</a>
      </div>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>Rank Distribution (Graph to compare 2 dates) 
          <br>*Number of Keywords per ranking Group</h5>
        </div>
        <div class="panel-body">
          <div id="chartContainer" style="height: 300px; width: 100%;"></div>
        </div>
        <div class="panel-footer">
          <p>Blue : Position 1-2</p> 
          <p>Yellow : Position 2-4</p> 
          <p>Green : 5-10</p>
          <p>Gray : 10-20</p> 
          <p>Light Gray : >20</p>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>CTR (only displaying queries with more than 100 impressions)</h5>
        </div>
        <div class="panel-body">
          <%= render "/shared/ctr_group"%>
        </div>
      </div>
    </div>
  </div>
</div> <!--Container -->


