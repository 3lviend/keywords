<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <div class="alert alert-success"> Selected Date: <%=  "#{params[:search][:from_1]}" ' - ' "#{params[:search][:to_1]}" %> </div>
      <h3>Compared to :</h3>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>Overview</h5>
        </div>
        <div class="panel-body">
          <%= render "/shared/table_overview" %>
        </div>
      </div>
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>Top Keywords (top 10 based on clicks)</h5>
        </div>
        <!-- /.panel-heading -->
        <div class="panel-body">
          <%= render "/shared/table_top_keywords" %>
          <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
   </div>
 </div>

 <div class="row">
   <div class="col-lg-12">
    <div class="col-md-6">
     <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>New Keywords (top 10 based on impressions)</h5>
        </div>
        <div class="panel-body">
           <%= render "/shared/table_top_keywords_new_impressions" %>
           <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
         <h5>Keywords disappeared (top 10 based on impressions from past period)</h5>
        </div>
      <div class="panel-body">
         <%= render "/shared/table_top_keywords_past_impressions" %>
         <a href="#" class="btn btn-primary">View More Link</a>
      </div>
      </div>
    </div>
  </div>
</div>

 <div class="row">
   <div class="col-lg-12">
    <div class="col-md-6">
     <div class="panel panel-primary">
        <div class="panel-heading">
           <h5>Improved Rankings (only displaying queries with more than 100 impressions) (top 10 based on average position improvement)</h5>
        </div>
        <div class="panel-body">
           <%= render "/shared/improved_rankings" %>
           <a href="#" class="btn btn-primary">View More Link</a>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
         <h5>Lost Rankings (only displaying queries with more than 100 impressions) (top 10 based on average position improvement)</h5>
        </div>
      <div class="panel-body">
         <%= render "/shared/lost_rankings" %>
         <a href="#" class="btn btn-primary">View More Link</a>
      </div>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>Rank Distribution (Graph to compare 2 dates) *Number of Keywords per ranking Group</h5>
        </div>
        <div class="panel-body">
          <div id="chartContainer" style="height: 300px; width: 100%;"></div>
        </div>
        <div class="panel-footer">
          <p>Blue : Position 1-2</p> 
          <p>Yellow : Position 2-4</p> 
          <p>Green : 5-10</p>
          <p>Gray : 10-20</p> 
          <p>Light Gray : >20</p>
        </div>
      </div>
    </div>
    <div class="col-md-6">
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h5>CTR (only displaying queries with more than 100 impressions)</h5>
        </div>
        <div class="panel-body">
          <div class="table-responsive">
           <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Position Group</th>
                    <th>CTR</th>
                    <th>CTR from previous period</th>
                    <th>Comparison</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <!-- data 1 -->
                    <td>1</td>
                    <td class="td1">22.5%</td>
                    <td></td>
                    <td>+1.%</td>
                </tr>
                <tr>
                    <!-- data 2 -->
                    <td>2</td>
                    <td class="td2">14%</td>
                    <td></td>
                    <td>-2%</td>
                </tr>
                <tr>
                    <!-- data 3 -->
                    <td>3</td>
                    <td class="td3">8.6%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 4 -->
                    <td>4</td>
                    <td class="td4">5.6%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 5 -->
                    <td>5</td>
                    <td class="td5">3.9%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 6 -->
                    <td>6</td>
                    <td class="td6">2%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 7 -->
                    <td>7</td>
                    <td class="td7">1.3%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 8 -->
                    <td>8</td>
                    <td class="td8">1%</td>
                    <td></td>
                    <td></td>
                </tr>
                <tr>
                    <!-- data 9 -->
                    <td>9</td>
                    <td class="td9">7%</td>
                    <td></td>
                    <td></td>
                </tr>
                </tbody>
              </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


