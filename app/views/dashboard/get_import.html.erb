<section class="import">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
         <h3>Import Keywords</h3>
         <div class="col-md-4">
          <%= form_tag keywords_path, multipart: true do %>
            <%= file_field_tag :file, class: "btn btn-default" %>
         </div>
         <div class="col-md-6">
          <%= submit_tag "Import", class: "btn btn-primary"%>
         </div>
        <% end %>
      </div>
    </div>
  </div>
</section>